{
  "name": "vue-shopping-cart",
  "version": "1.0.0",
  "description": "Simple shopping cart with Vue.js and Deno.js",
  "author": "Dang Van Thanh <dangvanthanh@dangthanh.org>",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev:client": "rollup -c -w",
    "dev:server": "deno run -A --reload --config=tsconfig.json server/app.ts",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\""
  },
  "dependencies": {
    "appwrite": "^1.1.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "cssnano": "^4.1.10",
    "esbuild": "0.8.24",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "postcss": "^7.0.33",
    "postcss-each": "^0.10.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "rollup": "2.35.1",
    "rollup-plugin-esbuild": "2.6.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-vue": "^5.1.9",
    "tailwindcss": "^1.9.4",
    "vue-template-compiler": "^2.6.12"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  }
}
