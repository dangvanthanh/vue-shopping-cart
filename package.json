{
  "name": "vue-shopping-cart",
  "version": "1.0.0",
  "description": "Simple shopping cart with Vue.js and Deno.js",
  "author": "Dang Van Thanh <dangvanthanh@dangthanh.org>",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev:client": "rollup -c -w",
    "dev:server": "deno run -A --unstable --reload --allow-write --allow-read --allow-plugin --allow-net --allow-env server/app.ts",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\""
  },
  "dependencies": {
    "concurrently": "^5.2.0",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "postcss-each": "^0.10.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "rollup": "^2.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-esbuild": "^2.4.2",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-vue": "^5.1.9",
    "tailwindcss": "^1.6.0",
    "vue-template-compiler": "^2.6.11"
  }
}
